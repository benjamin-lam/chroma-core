# ChromaDB Configuration Example
# Copy this file to .env and adjust as needed

# Path where ChromaDB stores data (default: ./chroma)
CHROMA_PATH=./chroma

# Default collection name (default: sample)
CHROMA_COLLECTION=sample

# Enable/disable anonymized telemetry (default: False)
ANONYMIZED_TELEMETRY=False

# Logging level: DEBUG, INFO, WARNING, ERROR, CRITICAL (default: INFO)
LOG_LEVEL=INFO
